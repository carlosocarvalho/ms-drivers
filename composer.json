{
    "name": "modalnetworks/metasearch-drivers",
    "license": "MIT",
    "keywords": [
        "modalnetworks", "acbd library"
    ],
    "authors": [
        {
            "name": "Carlos O Carvalho",
            "email": "cjc@modalnetworks.com"
        }
    ],
    "require": {
        "php": ">=7",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/support": "^5.7",
        "illuminate/database": "^5.7",
        "illuminate/events": "^5.7"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.2"
    },
    "autoload": {
        "psr-4":{
            "Modalnetworks\\MetaSearch\\":"src/"
        },
        "files": [
            "src/Helpers/abcd_helper.php",
            "src/Helpers/noticia_fiscal_helper.php",
            "src/Helpers/biosfera_helper.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {

      "laravel": {
          "providers": [
              "Modalnetworks\\MetaSearch\\Providers\\LaravelServiceProvider"
          ]
      }
    }
}
